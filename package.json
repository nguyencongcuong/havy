{
    "name": "havy",
    "description": "A minimal & elegant ghost theme from cuongnc.com",
    "version": "1.0.0",
    "engines": {
        "ghost": ">=3.0.0",
        "ghost-api": "v3"
    },
    "license": "MIT",
    "author": {
        "name": "Nguyen Cong Cuong",
        "email": "cuongnc.min@gmail.com"
    },
    "keywords": [
        "minimalism",
        "clean",
        "elegant",
        "ghost-theme"
    ],
    "config": {
        "posts_per_page": 10,
        "image_sizes": {
            "xxs": {
                "width": 30
            },
            "xs": {
                "width": 100
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1000
            },
            "xl": {
                "width": 2000
            }
        }
    },
    "devDependencies": {
        "autoprefixer": "^10.3.2",
        "browser-sync": "^2.27.5",
        "postcss": "^8.3.6",
        "tailwindcss": "^2.2.7"
    },
    "dependencies": {
        "@tailwindcss/aspect-ratio": "^0.2.1",
        "npm-git-install": "^0.3.0",
        "sass-convert": "^0.5.2"
    }
}
